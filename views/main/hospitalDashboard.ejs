<% layout('layout/layoutPlate') %>

<body class="bg-light">
        <div style="margin-top: 1rem;">

        </div>
        <!-- Header Placeholder -->
        <header class="bg-success text-white py-3">
            <div class="container text-center">
                <h1>Hospital Dashboard</h1>

            </div>
        </header>
    

    <div class="container my-4">
        <div class="text-center mb-4">
            <h1>Dashboard</h1>
            <p class="lead">Welcome to the Organ Donation Registration Platform</p>
        </div>

        <div class="text-center mb-3">
            <img src="data:image/png;base64," alt="Heart Icon" style="width: 80px;">
        </div>

        <div class="row g-4">
            <!-- Register as Donor -->
            <div class="col-md-6">
                <div class="card shadow-sm">
                    <div class="card-body">
                        <h5 class="card-title">Register as Donor</h5>
                        <p class="card-text">Join us to make a difference by registering as a donor.</p>
                        <button class="btn btn-success w-100" onclick="registerUser('Donor')">Register as Donor</button>
                    </div>
                </div>
            </div>

            <!-- Register as Receiver -->
            <div class="col-md-6">
                <div class="card shadow-sm">
                    <div class="card-body">
                        <h5 class="card-title">Register as Receiver</h5>
                        <p class="card-text">Sign up to receive organ donation support.</p>
                        <button class="btn btn-primary w-100" onclick="registerUser('Receiver')">Register as Receiver</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="mt-5">
            <h3 class="mb-3">Registered Users</h3>
            <div class="table-responsive">
                <table class="table table-striped table-bordered" id="data-table">
                    <thead class="table-dark">
                        <tr>
                            <th scope="col">ID</th>
                            <th scope="col">Name</th>
                            <th scope="col">Role</th>
                            <th scope="col">Status</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody id="data-body">
                        <!-- Dynamically populated data will appear here -->
                    </tbody>
                </table>
            </div>
        </div>

        <div class="mt-5">
            <h3>Credit Point System (Prototype)</h3>
            <p>Points will be awarded based on participation, successful donations, and community support initiatives.</p>
        </div>

                    <!-- Map and Dijkstra Algorithm Placeholder -->
        <div class="mt-5">
            <h3>Map & Route Optimization using Dijkstra Algorithm</h3>
            <p>This section includes a map for route optimization, helping to connect donors and receivers efficiently using the Dijkstra algorithm.</p>
            <!-- Map Image -->
            <div class="text-center">
                <img src="data:image/jpeg;base64," alt="Map for Dijkstra Algorithm" class="img-fluid">
            </div>
        </div>
    </div>

    
        </div>
    </div>
    
    <script>
        let userId = 1; // To keep track of user IDs
        const dataBody = document.getElementById('data-body'); // The tbody element to display data

        function registerUser(role) {
            const name = prompt("Please enter your name:");
            if (name) {
                const status = role === "Donor" ? "Active" : "Pending";
                const newRow = `
                    <tr id="user-${userId}">
                        <td>${userId}</td>
                        <td>${name}</td>
                        <td>${role}</td>
                        <td>${status}</td>
                        <td><button class="btn btn-danger btn-sm" onclick="deleteUser(${userId})">Delete</button></td>
                    </tr>
                `;
                dataBody.insertAdjacentHTML('beforeend', newRow); // Add the new row to the table
                userId++; // Increment user ID
            } else {
                alert("Name is required to register.");
            }
        }

        function deleteUser(id) {
            const row = document.getElementById(`user-${id}`);
            if (row) {
                row.remove(); // Remove the row from the table
            }
        }
    </script>
</body>
